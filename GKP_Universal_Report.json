    {
      "parameters": {
        "jsCode": "// Merge paginated results\nconst prev = $('Need More Pages?').first().json;\nconst response = $input.first().json;\nconst newResults = response.results || [];\nconst allResults = [...prev.allResults, ...newResults];\nconst nextPageToken = response.nextPageToken || null;\nconst limit = prev.limit;\n\nreturn [{\n  json: {\n    allResults: allResults,\n    nextPageToken: nextPageToken,\n    totalFetched: allResults.length,\n    limit: limit,\n    needMore: nextPageToken !== null && allResults.length < limit\n  }\n}];"
      },
      "id": "a1000001-0001-0001-0001-000000000009",
      "name": "Merge Pages",
      "type": "n8n-nodes-base.code",
      "typeVersion": 2,
      "position": [720, 200]
    },
    {
      "parameters": {
        "jsCode": "// Format GKP data into spreadsheet rows\nconst input = $input.first().json;\nconst allResults = input.allResults || [];\nconst limit = input.limit || 600;\nconst urlMapping = JSON.parse($('Set Variables').first().json.url_mapping || '{}');\nconst seedKeywords = JSON.parse($('Set Variables').first().json.seed_keywords || '[]');\n\n// Month names for headers\nconst monthNames = ['Jan', 'Feb', 'Mar', 'Apr', 'May', 'Jun', 'Jul', 'Aug', 'Sep', 'Oct', 'Nov', 'Dec'];\n\n// Parse each result\nconst parsed = allResults.map(r => {\n  const kw = r.text || '';\n  const metrics = r.keywordIdeaMetrics || {};\n  const avg = metrics.avgMonthlySearches ? parseInt(metrics.avgMonthlySearches) : 0;\n  const comp = metrics.competition || 'N/A';\n  const compIndex = metrics.competitionIndex !== undefined ? metrics.competitionIndex : '';\n  \n  // Monthly volumes\n  const monthlyVolumes = (metrics.monthlySearchVolumes || []).map(m => {\n    return m.monthlySearches ? parseInt(m.monthlySearches) : 0;\n  });\n  \n  // Month headers from actual data\n  const monthHeaders = (metrics.monthlySearchVolumes || []).map(m => {\n    const monthIdx = parseInt(m.month) - 1;\n    return monthNames[monthIdx] + ' ' + m.year;\n  });\n  \n  // Find matching URL\n  const url = urlMapping[kw] || '';\n  \n  // Determine which seed keyword this belongs to\n  let seedGroup = 'Other';\n  for (const seed of seedKeywords) {\n    if (kw.toLowerCase().includes(seed.toLowerCase()) || seed.toLowerCase().includes(kw.toLowerCase())) {\n      seedGroup = seed;\n      break;\n    }\n  }\n  \n  return {\n    keyword: kw,\n    url: url,\n    avgMonthlySearches: avg,\n    competition: comp,\n    competitionIndex: compIndex,\n    monthlyVolumes: monthlyVolumes,\n    monthHeaders: monthHeaders,\n    seedGroup: seedGroup\n  };\n});\n\n// Sort by avg monthly searches desc\nparsed.sort((a, b) => b.avgMonthlySearches - a.avgMonthlySearches);\n\n// Limit\nconst limited = parsed.slice(0, limit);\n\n// Group by seed keyword\nconst groups = {};\nfor (const item of limited) {\n  if (!groups[item.seedGroup]) {\n    groups[item.seedGroup] = [];\n  }\n  groups[item.seedGroup].push(item);\n}\n\n// Get month headers from first result\nconst monthHeaderRow = limited.length > 0 ? limited[0].monthHeaders : [];\n\n// Build sheet data rows\nconst headerRow = ['Keyword', 'URL', 'Avg. monthly searches', 'Competition', 'Competition (indexed value)', ...monthHeaderRow];\n\nconst rows = [headerRow];\n\nfor (const [seedName, items] of Object.entries(groups)) {\n  // Cluster header\n  const emptyRow = new Array(headerRow.length).fill('');\n  rows.push(emptyRow);\n  \n  const clusterHeader = new Array(headerRow.length).fill('');\n  clusterHeader[0] = `Cluster: ${seedName}`;\n  rows.push(clusterHeader);\n  \n  // Data rows\n  for (const item of items) {\n    rows.push([\n      item.keyword,\n      item.url,\n      item.avgMonthlySearches,\n      item.competition,\n      item.competitionIndex,\n      ...item.monthlyVolumes\n    ]);\n  }\n  \n  // Cluster summary\n  const clusterTotal = items.reduce((sum, i) => sum + i.avgMonthlySearches, 0);\n  const summaryRow = new Array(headerRow.length).fill('');\n  summaryRow[0] = 'Cluster monthly searches:';\n  summaryRow[2] = clusterTotal;\n  rows.push(summaryRow);\n}\n\nreturn [{\n  json: {\n    rows: rows,\n    totalKeywords: limited.length,\n    totalGroups: Object.keys(groups).length,\n    sheetId: $('Create Spreadsheet').first().json.spreadsheetId || $('Create Spreadsheet').first().json.id\n  }\n}];"
      },
